  "repository": {
    "comments": {
      "patterns": [
        {
          "name": "comment.block.asl",
          "begin": "/\\*",
          "beginCaptures": {
            "0": {
              "name": "punctuation.definition.comment.asl"
            }
          },
          "end": "\\*/",
          "endCaptures": {
            "0": {
              "name": "punctuation.definition.comment.asl"
            }
          }
        },
        {
          "begin": "(^\\s+)?(?=//)",
          "beginCaptures": {
            "1": {
              "name": "punctuation.whitespace.comment.leading.cs"
            }
          },
          "end": "(?=$)",
          "name": "comment.line.states"
        }
      ]
    }
  }
